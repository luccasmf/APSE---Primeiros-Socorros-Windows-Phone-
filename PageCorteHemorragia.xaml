<Page
    x:Class="PhoneAppPI.PageCorteHemorragia"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PhoneAppPI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Grid.Background>
            <ImageBrush Stretch="Fill" ImageSource="Assets/fundo_03.jpg"/>
        </Grid.Background>
        <Ellipse HorizontalAlignment="Left" Height="100                  " Margin="30,56,0,0" Stroke="Black" VerticalAlignment="Top" Width="100" RequestedTheme="Dark" StrokeThickness="0">
            <Ellipse.Fill>
                <ImageBrush Stretch="UniformToFill" ImageSource="Assets/Icone.png"/>
            </Ellipse.Fill>
        </Ellipse>
        <TextBlock HorizontalAlignment="Left" Margin="151,67,0,0" TextWrapping="Wrap" Text="Hemorragia Externa" VerticalAlignment="Top" Height="89" Width="215" SelectionHighlightColor="#FFF7F7F9" FontSize="35" FontWeight="Bold"/>

    </Grid>
</Page>
